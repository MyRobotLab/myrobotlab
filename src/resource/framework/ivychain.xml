<!-- refer to : http://ant.apache.org/ivy/history/2.2.0/tutorial/defaultconf.html -->
<ivysettings>
   <settings defaultResolver="public"/>
   	<resolvers>
   	  <chain name="public">
	  	  	
		<url name="public-remote">
		  <ivy pattern="https://raw.githubusercontent.com/MyRobotLab/repo/{release}/[organisation]/[revision]/ivy.xml"/>
		  <artifact pattern="https://raw.githubusercontent.com/MyRobotLab/repo/{release}/[organisation]/[revision]/[artifact].[ext]"/>
		</url>
	  </chain>
	</resolvers>
</ivysettings>	
